<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ncic="http://cjis.fbi.gov/ncic/NIEM" xmlns:ncic-ex="http://cjis.fbi.gov/ncic/NIEM/Exchange" xmlns:s="http://release.niem.gov/niem/structures/3.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/" xmlns:m="http://release.niem.gov/niem/domains/maritime/3.0/" xmlns:niem-xsd="http://release.niem.gov/niem/proxy/xsd/3.0/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/5.0/" xmlns:scr="http://release.niem.gov/niem/domains/screening/3.0/" xmlns:it="http://release.niem.gov/niem/domains/internationalTrade/3.0/" xmlns:cbrn="http://release.niem.gov/niem/domains/cbrn/3.0/" targetNamespace="http://cjis.fbi.gov/ncic/NIEM">
	<xsd:import namespace="http://release.niem.gov/niem/structures/3.0/" schemaLocation="../../../common/niem/structures/3.0/structures.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Defined structures for creating types.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/niem-core/3.0/" schemaLocation="../../../common/niem/niem-core/3.0/niem-core.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Core Data Model.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/domains/maritime/3.0/" schemaLocation="../../../common/niem/domains/maritime/3.0/maritime.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Maritime Data Model.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/domains/jxdm/5.0/" schemaLocation="../../../common/niem/domains/jxdm/5.0/jxdm.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Domain Schema, Justice Exchange, containing The base Justice
                information.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/proxy/xsd/3.0/" schemaLocation="../../../common/niem/proxy/xsd/3.0/xs.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM XML Proxy types.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/domains/screening/3.0/" schemaLocation="../../../common/niem/domains/screening/3.0/screening.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Domain Schema, Screening Portfolio of DHS, containing The base
                Screening information.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/domains/internationalTrade/3.0/" schemaLocation="../../../common/niem/domains/internationalTrade/3.0/internationalTrade.xsd">
		<xsd:annotation>
			<xsd:documentation>NIEM Domain Schema, International Trade.</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<xsd:import namespace="http://release.niem.gov/niem/domains/cbrn/3.0/" schemaLocation="../../../common/niem/domains/cbrn/3.0/cbrn.xsd">
		<xsd:annotation>
			<xsd:documentation>Chemical, Biological, Radiological, and Nuclear
                Domain</xsd:documentation>
		</xsd:annotation>
	</xsd:import>
	<!-- ********************************************************* -->
	<!--A-->
	<xsd:complexType name="ActivityAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about an
                activity.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:CaseNumberText" minOccurs="0"/>
					<xsd:element ref="ncic:ControlNumberID" minOccurs="0"/>
					<!-- Truncated for ncic:RecordValidationDate
                    <xsd:element ref="ncic:DocumentValidationDate" minOccurs="0"/>
                    -->
					<xsd:element ref="ncic:RecordValidationDate" minOccurs="0"/>
					<xsd:element ref="ncic:StopPlacedDate" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ActivityCaseOrganizationAssociationType">
		<xsd:annotation>
			<xsd:documentation>A data type for an association between an Activity, Case, and
                Organization.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Activity" minOccurs="0"/>
					<xsd:element ref="nc:Case" minOccurs="0"/>
					<xsd:element ref="nc:Incident" minOccurs="0"/>
					<xsd:element ref="nc:Organization" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about an address.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<!-- Truncated. Moved to scr:PersonAddressAugmentation
                    <xsd:element ref="ncic:AddressCategoryCodeText" minOccurs="0"/>                   
                    <xsd:element ref="ncic:AddressStartDate" minOccurs="0"/>
                    <xsd:element ref="ncic:AddressEndDate" minOccurs="0"/>
                    -->
					<xsd:element ref="ncic:DocumentedAddressDate" minOccurs="0"/>
					<xsd:element ref="ncic:ForiegnPostalCodeText" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ArticleRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure the data in an article
                record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:ItemRecordType">
				<xsd:sequence>
					<xsd:element ref="nc:Item" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:ImageItem" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:PublicDisseminationDateText" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- B -->
	<xsd:complexType name="BenefitsItemOffenseSummaryAssociationType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure the data in a benefits item and offense
                summary association.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Item"/>
					<xsd:element ref="j:OffenseSummary"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BoatRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure the data in a boat
                record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:ConveyanceRecordType">
				<xsd:sequence>
					<xsd:element ref="ncic:VesselRegistrationImageAssociation" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- C -->
	<xsd:complexType name="CaseAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about a case.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:LocationStateNCICLSTACode" minOccurs="0"/>
					<xsd:element ref="j:SentenceDate" minOccurs="0"/>
					<xsd:element ref="ncic:NICSTransactionID" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CaseRemovalIncidentAssociationType">
		<xsd:annotation>
			<xsd:documentation>A data type for an association between a removal activity, case and
                incident.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:Activity" minOccurs="0"/>
					<xsd:element ref="nc:Case" minOccurs="0"/>
					<xsd:element ref="nc:Incident" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ContactInformationAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional contact information</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:InternetID"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConveyancePartAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional conveyance part
                information.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:EngineHorsepowerQuantity"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CourtOrderAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional court order
    information.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:CourtOrderServiceStatus" minOccurs="0" maxOccurs="2"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CriminalOrganizationAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional criminal organization
                information.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:ImageNCICIdentification"/>
					<xsd:element ref="j:ImageCategoryCode"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CommonRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure common record data that identifies a
                specific record in NCIC.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:RecordType">
				<xsd:sequence>
					<xsd:element ref="j:OffenseSubjectAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:OffenseSummary" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:PersonNCICIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ProtectionOrder" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:RegisteredOffender" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- D -->
	<xsd:complexType name="DatabaseRecordStatusCodeType">
		<xsd:annotation>
			<xsd:documentation>A data type for an NCIC database record status.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ncic:DatabaseRecordStatusCodeSimpleType">
				<xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="DetainmentType">
		<xsd:annotation>
			<xsd:documentation>A data type for the detainment of a wanted
                person.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:ActivityType">
				<xsd:sequence>
					<xsd:element ref="j:Incarceration" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Sentence" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--E-->
	<xsd:complexType name="EntityAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about an entity.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<!-- Truncated for ncic:RecordValidationDate
                    <xsd:element ref="ncic:DocumentValidationDate" minOccurs="0"
                        maxOccurs="unbounded"/>
                    -->
					<xsd:element ref="ncic:RecordValidationDate" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExtendedBirthdaySearchCodeType">
		<xsd:annotation>
			<xsd:documentation>A data type for Extended Birthday Search Type.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="ncic:ExtendedBirthdaySearchCodeSimpleType">
				<xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- G -->
	<xsd:complexType name="GunRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure data in a gun record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:ItemRecordType">
				<xsd:sequence>
					<xsd:element ref="j:Firearm" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--H-->
	<!--I-->
	<xsd:complexType name="IdentityTheftRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure data in a identity theft
                record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:PersonRecordType">
				<xsd:sequence>
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ImageRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure data for an image
                record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:RecordType">
				<xsd:sequence>
					<xsd:element ref="j:ImageNCICIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ItemNCICIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:PersonNCICIdentification" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ConveyanceRegistrationAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:CriminalOrganization" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:Item" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ItemImage" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:Vehicle" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:Vessel" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:ConveyancePart" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:BinaryDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:ImagePerson" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="InvestigativeInterestType">
		<xsd:annotation>
			<xsd:documentation>A data type for to contain investigative
                interests.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="nc:IncidentType">
				<xsd:sequence>
					<xsd:element ref="nc:Organization" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IncidentAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about an
                incident.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:PersonPasswordText" minOccurs="0"/>
					<xsd:element ref="ncic:NICSTransactionID" minOccurs="0"/>
					<xsd:element ref="ncic:PurgeDate" minOccurs="0"/>
					<xsd:element ref="ncic:ReleaseDate" minOccurs="0"/>
					<xsd:element ref="ncic:ViolationDate" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ItemAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional data about an item.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="it:CommodityUNDGIdentificationText"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SupplementalRecordAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about an item registration
                association.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:FraudulentRecordIndicator" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:SupplementalRecordIndicator" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- L -->
	<xsd:complexType name="LicensePlateRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure license plate record
                data.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:ItemRecordType">
				<xsd:sequence>
					<xsd:element ref="j:ConveyanceRegistration" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- Deprecated element, was used by ARE
    <xsd:complexType name="LocationAugmentationPointType">
        <xsd:annotation>
            <xsd:documentation>A data type to structure additional location
                information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="s:ObjectType">
                <xsd:sequence>
                    <xsd:element ref="ncic:MissingPersonAreaText"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
-->
	<!--M-->
	<xsd:complexType name="MessageInterfacingOrganizationType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure data in a NCIC Message.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="j:OrganizationORIIdentification" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MissingPersonRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure the data pertaining to a NCIC missing person
                record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:PersonRecordType">
				<xsd:sequence>
					<xsd:element ref="nc:ItemEntityAssociation" minOccurs="0"/>
					<!-- <xsd:element ref="nc:RoleOfPerson" minOccurs="0"/> -->
					<xsd:element ref="j:MissingPerson" minOccurs="0"/>
					<xsd:element ref="ncic:PersonMatchData" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:PersonWithInformation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:PublicDisseminationDateText" minOccurs="0"/>
					<xsd:element ref="ncic:RecoveredItemOffenseSummaryAssociation" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--N-->
	<xsd:complexType name="NICSDeniedRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type to structure the NICS Denied record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:PersonRecordType">
				<xsd:sequence>
					<xsd:element ref="nc:PersonResidenceAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="j:Subject" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--O-->
	<xsd:complexType name="OffenseAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about an
                Offense.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:AdditionalOffenseIndictator" minOccurs="0"/>
					<xsd:element ref="ncic:OriginalOffense" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OffenseSummaryAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for addConveyanceRegistrationAssociationitional
                information about an offense summary.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="nc:PersonArmedIndicator" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="j:SubjectArmedIndicator" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="j:PersonHitResultsText" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="ncic:OffenseSummaryPersonsFoundQuantity" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OrganizationAugmentationType">
		<xsd:annotation>
			<xsd:documentation>A data type for additional information about an
                organization.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:OrganizationDocumentCount"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OrganizationDocumentCountType">
		<xsd:annotation>
			<xsd:documentation>A data type for organization document counts.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
				<xsd:sequence>
					<xsd:element ref="ncic:CodeManualCount"/>
					<xsd:element ref="ncic:NewsletterCount"/>
					<xsd:element ref="ncic:OperatingManualCount"/>
					<xsd:element ref="ncic:TOUCount"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ORIRecordType">
		<xsd:annotation>
			<xsd:documentation>A data type for information about an ORI record.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ncic:RecordType">
				<xsd:sequence>
					<xsd:element ref="j:OffenseSubjectAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ContactEntity" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="nc:ContactInformationAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ncic:UserCodeID" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OriginalOffenseType">
		<xsd:annotation>
			<xsd:documentation>A data type for information about a record's original
                offense.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="s:ObjectType">
