	<xsd:simpleType name="DatabaseRecordStatusCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>A data type for an NCIC database record status.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="LOCATED">
				<xsd:annotation>
					<xsd:documentation>LOCATED</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CLEARED">
				<xsd:annotation>
					<xsd:documentation>CLEARED</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CANCELED">
				<xsd:annotation>
					<xsd:documentation>CANCELED</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--E-->
	<xsd:simpleType name="ExtendedBirthdaySearchCodeSimpleType">
		<xsd:annotation>
			<xsd:documentation>A data type for Extended Birthday Search Type.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>An exact match on the birth date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>A swap of the month and date.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>One year increment/decrement of the birth
                        year.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
