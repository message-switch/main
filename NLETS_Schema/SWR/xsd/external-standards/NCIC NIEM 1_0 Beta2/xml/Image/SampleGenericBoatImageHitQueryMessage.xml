<?xml version="1.0" encoding="UTF-8"?>
<ncic-ex:Exchange xmlns:ncic="http://cjis.fbi.gov/ncic/NIEM"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:nc="http://release.niem.gov/niem/niem-core/3.0/"
