<?xml version="1.0" encoding="UTF-8"?>
<sl:RegistryConfig xmlns:sl="http://www.gcomsoft.com/schema/fwk/sl/v1-0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.gcomsoft.com/schema/fwk/sl/v1-0 http://www.gcomsoft.com/schema/fwk/sl/v1-0 ">
	<sl:Contexts>
		<sl:Context ServerName="CURRENT_CONTEXT">
		</sl:Context>
	</sl:Contexts>
	
	<sl:AppRegistries>
		
		<!-- FRAMEWORKS -->
		<sl:AppRegistry AppName="GCOM_COMMON_LIB">
			<sl:EmailSessions>
				<sl:EmailSession>
					<sl:AliasName>ExceptionReportEmail</sl:AliasName>
					<sl:JNDIName>java:jboss/mail/Default</sl:JNDIName>
				</sl:EmailSession>
				<sl:EmailSession>
					<sl:AliasName>NotificationEmail</sl:AliasName>
					<sl:JNDIName>java:jboss/mail/Default</sl:JNDIName>
				</sl:EmailSession>
			</sl:EmailSessions>
		</sl:AppRegistry>
		
		
		<sl:AppRegistry AppName="OSMS_MESSAGE_LOG_SERVICES">
			<sl:EJBServices>
				<sl:EJBService PreferLocal="false">
					<sl:AliasName>com.gcom.osms.messagelog.service.OSMSMessageLogService</sl:AliasName>
					<sl:LocalName>java:global/OSMSMessageLogEAR/OSMSMessageLogFacade/OSMSMessageLogEJB!com.gcom.osms.messagelog.service.OSMSMessageLogServiceLocal</sl:LocalName>
					<sl:RemoteName>java:jboss/exported/OSMSMessageLogEAR/OSMSMessageLogFacade/OSMSMessageLogEJB!com.gcom.osms.messagelog.service.OSMSMessageLogServiceRemote</sl:RemoteName>
				</sl:EJBService>
			</sl:EJBServices>
			<sl:DataSources>
				<sl:DataSource>
					<sl:AliasName>MessageLogDS</sl:AliasName>
					<sl:JNDIName>java:jboss/datasources/MessageLogDS</sl:JNDIName>
				</sl:DataSource>
			</sl:DataSources>
		</sl:AppRegistry>

		
	</sl:AppRegistries>
</sl:RegistryConfig>
